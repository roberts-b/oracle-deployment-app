{
  "name": "oracle-deployment-app",
  "version": "0.0.1",
  "description": "App to manage development and deployments with Oracle DB",
  "author": "Robik",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds --overwrite",
    "rebuild": "electron-rebuild -v 1.7.8 --force -m . -w oracledb"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.1",
    "electron": "1.7.8",
    "electron-packager": "^9.1.0",
    "electron-rebuild": "^1.6.0",
    "eslint": "^4.7.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.74.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "electron-log": "^2.2.9",
    "electron-settings": "^3.1.2",
    "oracledb": "^1.13.1",
    "tns": "^0.1.0"
  }
}
